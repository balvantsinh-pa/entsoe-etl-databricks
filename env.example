# ENTSOE API Configuration
# Get your API key from: https://transparency.entsoe.eu/content/static_content/Static%20content/web%20api/Guide.html
ENTSOE_API_KEY=your_entsoe_api_key_here

# Database Configuration
# Format: postgresql://username:password@host:port/database
DATABASE_URL=postgresql://username:password@localhost:5432/entsoe_data

# Country Configuration (Germany by default)
COUNTRY_EIC=10Y1001A1001A82H
COUNTRY_CODE=DE

# Logging Configuration
LOG_LEVEL=INFO

# API Configuration
MAX_RETRIES=3
RETRY_DELAY=5
REQUEST_TIMEOUT=30 